net: "models/darknet_yolov2/train_val-voc.2.0_step1.prototxt"
test_iter: 309   # * batch = test_size  batch_size = 16
test_interval: 500 # * batch = train_size
test_initialization: false

base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 1000
max_iter: 5000

display: 20
momentum: 0.9
weight_decay: 0.0005
snapshot: 500
snapshot_prefix: "models/darknet_yolov2/yolo-voc_step2"
solver_mode: GPU